{
  "data": {
    "data_size": "100k",
    "format": "modular"
  },
  "model": {
    "K": 30,
    "width": 128,
    "n_blocks": 8,
    "neck_width": 64,
    "dropout_rate": 0.0,
    "l2_reg": 1e-05
  },
  "pca": {
    "K": 30,
    "use_scaler": true
  },
  "loss": {
    "delta": 0.015,
    "alpha": 0.1,
    "beta": 0.05,
    "grid_shape": [
      4,
      15
    ]
  },
  "train": {
    "epochs": 200,
    "batch_size": 256,
    "learning_rate": 0.001,
    "patience": 10,
    "otm_put_weight": 2.0
  }
}